# Lady QR - Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=ladyqr.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=ladyqr
VITE_FIREBASE_STORAGE_BUCKET=ladyqr.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here

# Stripe Configuration (for payments)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_APP_URL=http://localhost:5173

# Development Settings
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=true

# Email Configuration (for contact forms)
VITE_CONTACT_EMAIL=support@ladyqr.com

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_STRIPE_PAYMENTS=true
VITE_ENABLE_QR_ANALYTICS=true